{
  "name": "owl-ui.v2",
  "version": "2.0.0",
  "description": "OWL UI Components",
  "main": "dist/owl-ui.min.js",
  "scripts": {
    "dev": "babel-node ./scripts/dev",
    "build": "NODE_ENV=production babel-node scripts/build",
    "build.styl": "babel-node ./scripts/build-styl",
    "build.demo": "WEBPACK=demo babel-node ./scripts/build-demo",
    "build.svg": "babel-node ./scripts/build-svg",
    "xo": "xo",
    "test": "nyc ava",
    "test.watch": "nyc ava -w & babel-node ./scripts/test-report",
    "test.report": "nyc report --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "node ./scripts/postinstall"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/jsdom",
      "./test/global",
      "./src/demo/server/require-hooks"
    ],
    "babel": "inherit",
    "files": [
      "./src/components/**/__test__/*.js"
    ]
  },
  "nyc": {
    "exclude": [
      "webpack.config.js",
      "src/demo/server/require-hooks.js",
      "src/components/**/__test__/*.js",
      "test"
    ]
  },
  "author": "Rocky Wu <rwu@cepave.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "ava": "^0.15.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.12.12",
    "cash-dom": "^1.3.0",
    "chalk": "^1.1.3",
    "clipboard": "^1.5.10",
    "codemirror": "^5.15.2",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "enzyme": "^2.3.0",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-plugin-ava": "^2.4.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-clean-css": "^2.0.10",
    "gulp-ftp": "^1.1.0",
    "gulp-if": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-stylus": "^2.4.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "jsdom": "^9.2.1",
    "lodash.pick": "^4.2.1",
    "normalize.css": "^4.1.1",
    "nyc": "^6.4.4",
    "piping": "^0.3.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.1",
    "redux-thunk": "^2.1.0",
    "require-hooks": "0.0.6",
    "rucksack-css": "^0.8.5",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "stop.js": "^0.2.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "why-did-you-update": "0.0.8",
    "xo": "^0.15.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "kouto-swiss": "^0.12.0",
    "lodash.flatten": "^4.2.0",
    "lodash.isequal": "^4.2.0",
    "react": "^15.1.0",
    "react-addons-text-content": "0.0.4",
    "react-dom": "^15.1.0",
    "react-tag": "^2.2.5"
  },
  "xo": {
    "ignores": [
      "static/*",
      "test/*"
    ],
    "esnext": true,
    "space": [
      true,
      2
    ],
    "semicolon": false,
    "extends": "xo-react",
    "rules": {
      "ava/use-t": 0,
      "ava/no-ignored-test-files": 0,
      "default-case": 0,
      "no-empty-pattern": 0,
      "arrow-spacing": 0,
      "spaced-comment": 0,
      "comma-dangle": 0,
      "no-undef": 0,
      "operator-linebreak": 0,
      "indent": 0,
      "babel/arrow-parens": 0,
      "react/jsx-space-before-closing": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-indent-props": 0,
      "react/jsx-no-bind": 0,
      "react/react-in-jsx-scope": 0,
      "react/no-string-refs": 0,
      "react/jsx-handler-names": 0,
      "react/prop-types": 0,
      "react/jsx-first-prop-new-line": 0,
      "babel/object-curly-spacing": 0,
      "no-unused-vars": 0,
      "quote-props": 0,
      "no-alert": 0,
      "import-order/import-order": 0,
      "no-multiple-empty-lines": 0
    }
  }
}
