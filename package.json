{
  "name": "owl-ui",
  "version": "0.0.4",
  "description": "OWL UI Framework",
  "main": "build/components",
  "scripts": {
    "dev": "babel-node ./scripts/dev",
    "build.styl": "babel-node ./scripts/build.styl",
    "build.svg": "babel-node ./scripts/build.svg",
    "test": "nyc ava && nyc report --reporter=html",
    "test.watch": "nyc ava -w",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "babel-node ./scripts/postinstall"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/jsdom",
      "./test/global",
      "./src/server/require-hooks"
    ],
    "babel": "inherit",
    "files": [
      "./src/components/**/__test__/*.js"
    ]
  },
  "nyc": {
    "exclude": [
      "webpack.config.js",
      "src/server/require-hooks.js",
      "test/**"
    ]
  },
  "author": "Rocky Wu <rwu@cepave.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "ava": "^0.14.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.12.8",
    "cash-dom": "^1.3.0",
    "chalk": "^1.1.3",
    "clipboard": "^1.5.10",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "enzyme": "^2.3.0",
    "eslint": "2.9.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
