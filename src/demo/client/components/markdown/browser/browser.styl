.logo
  display: inline-block

@font-face {font-family: "browser";
  src: url('./font/browser.eot'); /* IE9*/
  src: url('./font/browser.eot') format('embedded-opentype'), /* IE6-IE8 */
    url('./font/browser.woff') format('woff'), /* chrome, firefox */
    url('./font/browser.ttf') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('./font/browser.svg') format('svg'); /* iOS 4.1- */
}

.browser {
  font-family: "browser";
  font-size: 60px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.chrome:after { content: "\e60e"; }
.safari:after { content: "\e61b"; }
.ie:after { content: "\e6bf"; }
.firefox:after { content: "\e617"; }
.opera:after { content: "\e619"; }
.edge:after { content: "\e7de"; }
.android:after { content: "\e604"; }
